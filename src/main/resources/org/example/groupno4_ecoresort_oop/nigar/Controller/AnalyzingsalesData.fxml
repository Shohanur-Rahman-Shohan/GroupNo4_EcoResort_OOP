<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="497.0" prefWidth="702.0"
            xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.groupno4_ecoresort_oop.nigar.AnalyzingSalesDataController">
    <children>
        <TableView fx:id="tableView" layoutX="178.0" layoutY="128.0" prefHeight="208.0" prefWidth="367.0">
            <columns>
                <TableColumn fx:id="dateColumn" prefWidth="106.4" text="Date"/>
                <TableColumn fx:id="revenueColumn" prefWidth="130.4" text="Revenue"/>
                <TableColumn fx:id="salesColumn" prefWidth="130.4" text="Sales"/>
            </columns>
        </TableView>

        <Button fx:id="report" layoutX="479.0" layoutY="385.0" mnemonicParsing="false"
                onAction="#setNotification"
                style="-fx-background-radius: 30; -fx-background-color: #ff00ff;"
                text="Report"/>

        <Label layoutX="314.0" layoutY="44.0" text="Analyzing Sales Data" textFill="#692f5c">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
